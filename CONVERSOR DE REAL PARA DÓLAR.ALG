algoritmo "Conversor de reais pra dólar"
var
   Reais, Dolares: Real
inicio
      Escreval ("Dezembro/2023 (data de criação do algoritmo), o dólar vale 4,88.")
      Escreva ("Digite seu valor em reais: ")
      Leia (Reais)
      Dolares <- reais/4.88
      Escreva ("você pode ter", Dolares, (" dólares"))
fimalgoritmo