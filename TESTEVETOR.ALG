algoritmo "TesteVetor"
var
   V: Vetor[1..10] de Inteiro
   C: Inteiro
inicio
      Para C <- 1 ate 10 faca
           Escreva ("Digite o ", C, "° valor: ")
           Leia (V[c])
      FimPara
      
      Para C <- 1 ate 10 faca
           Escreva ("{", V[c], "}")
      FimPara
fimalgoritmo